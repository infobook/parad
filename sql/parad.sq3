SELECT
	[AOGUID] AS [cGUID]
	,[REGIONCODE] AS [cRegCode]
	,[SHORTNAME]  AS [cSName]
    ,[FORMALNAME] AS [cFName]
    ,UCase([FORMALNAME]) AS [cUName]
	,[ACTSTATUS] AS [cStatus]
	,[LIVESTATUS] AS [cLive]
	,[AOLEVEL] AS [cLevel]
	,0 AS [cIsLoadNext]
FROM
	[ADDROBJ]
WHERE
	[PARENTGUID] is null	
ORDER BY 
	[FORMALNAME], [LIVESTATUS] desc
</Text>
