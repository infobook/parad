      <Text>SELECT 
	AO1.[AOGUID]
	,AO1.[SHORTNAME] 
	,AO1.[FORMALNAME]
	,AO1.[AOLEVEL]
	
	,AO2.[AOGUID]
	,AO2.[SHORTNAME] 
	,AO2.[FORMALNAME]
	,AO2.[AOLEVEL]

	,AO3.[AOGUID]
	,AO3.[SHORTNAME] 
	,AO3.[FORMALNAME]
	,AO3.[AOLEVEL]
FROM 
	([ADDROBJ] AS AO1
		LEFT JOIN [ADDROBJ] AS AO2 ON AO1.[PARENTGUID] = AO2.[AOGUID])
		LEFT JOIN [ADDROBJ] AS AO3 ON AO2.[PARENTGUID] = AO3.[AOGUID]
WHERE
	AO1.[POSTALCODE] LIKE '#?index?#'
</Text>
