      <Text>SELECT
	[AOGUID] AS [aGUID]
	,[SHORTNAME]  AS [aSName]
    ,[FORMALNAME] AS [aFName]
    ,UCase([FORMALNAME]) AS [cUName]

	,[ACTSTATUS]  AS [aStatus]
	,[LIVESTATUS] AS [cLive]
	,[AOLEVEL] AS [aLevel]
	
     ,[REGIONCODE] AS [aRegCode] 
     ,[AREACODE] AS [aAreaCode]	
     ,[CITYCODE] AS [aCityCode]	
     ,[PLACECODE] AS [aPlaceCode]	 
	 ,[PLAINCODE] AS [aPlainCode]
     ,[STREETCODE] AS [aStreetCode]	

     ,[CTARCODE]	
     ,[EXTRCODE]	
     ,[SEXTCODE]

FROM
	[ADDROBJ]
WHERE
	[AOLEVEL] in (#?GetLevel?#)
	AND [REGIONCODE] = '#?RegCode?#'
ORDER BY 
	[FORMALNAME], [LIVESTATUS] desc
</Text>
      <XSLT />
      <ImageName />
      <Params>
        <Param>
          <Number>2</Number>
          <Title>Код региона</Title>
          <Name>RegCode</Name>
          <Type>String</Type>
          <Inset>true</Inset>
          <CurrentValue>77</CurrentValue>
          <DefaultValue />
          <SelectValue />
        </Param>
        <Param>
          <Number>4</Number>
          <Title>Уровни (3,4)</Title>
          <Name>GetLevel</Name>
          <Type>String</Type>
          <Inset>true</Inset>
          <CurrentValue>3,35,4,5,6,65,7</CurrentValue>
          <DefaultValue />
          <SelectValue />
        </Param>
      </Params>
    </Query>
    <Query>
